{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@/app/*": ["./app/*"],
      "@/core/*": ["./core/*"],

      "@/public/*": ["./public/*"],
      "@/images/*": ["./public/images/*"],
      "@/components/*": ["./core/components/*"],
      "@/templates/*": ["./core/components/templates/*"],
      "@/modules/*": ["./core/components/modules/*"],
      "@/layout/*": ["./core/components/layout/*"],
      "@/elements/*": ["./core/components/elements/*"],
      "@/constants/*": ["./core/constants/*"],
      "@/models/*": ["./core/models/*"],
      "@/helper/*": ["./core/helper/*"],
      "@/utils/*": ["./core/utils/*"],
      "@/theme/*": ["./core/theme/*"],
      "@/provider/*": ["./core/provider/*"],
      "@/loader/*": ["./core/components/loader/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts","global.d.ts"],
  "exclude": ["node_modules"]
}
